{
    "version": 2,
    "routes": [{
            "src": "/api/auth/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/user/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/post/(.*)",
            "dest": "/post/index-post.js"
        }
    ],
    "builds": [{
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "post/index-post.js",
            "use": "@vercel/node"
        },
        {
            "src": "user/index.js",
            "use": "@vercel/node"
        }
    ]
}